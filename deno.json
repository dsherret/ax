{
  "name": "@david/dax",
  "version": "0.0.0",
  "lock": false,
  "tasks": {
    "test": "deno test -A",
    "wasmbuild": "deno run -A https://deno.land/x/wasmbuild@0.15.6/main.ts --sync --out ./src/lib"
  },
  "fmt": {
    "proseWrap": "preserve",
    "lineWidth": 120
  },
  "lint": {
    "rules": {
      "exclude": ["ban-types", "no-explicit-any", "no-this-alias"],
      "include": [
        "no-console",
        "no-sync-fn-in-async-fn"
      ]
    }
  },
  "exclude": [
    ".gitattributes",
    ".gitignore",
    ".github",
    ".rustfmt.toml",
    ".vscode",
    "Cargo.toml",
    "Cargo.lock",
    "npm/",
    "target/"
  ],
  "exports": "./mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@0.213.0",
    "@std/io/": "jsr:/@std/io@0.213.0/",
    "which_runtime": "jsr:/@david/which-runtime@0.2",
    "@std/fmt/": "jsr:/@std/fmt@0.213.0/",
    "@std/fs": "jsr:@std/fs@0.213.0",
    "@std/path": "jsr:@std/path@0.213.0",
    "@std/streams/": "jsr:/@std/streams@0.213.0/",
    "which": "jsr:@david/which@0.3"
  }
}
